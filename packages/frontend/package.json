{
  "name": "@zammad-coding-test/frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint:path": "eslint --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint": "pnpm run lint:path -- .",
    "generate:graphql:codegen": "graphql-codegen-esm",
    "generate:graphql:lint": "pnpm run lint:path -- src/generated/graphql.ts",
    "generate:graphql": "run-s generate:graphql:codegen generate:graphql:lint",
    "dev:generate:graphql": "nodemon --config nodemon.graphql.json",
    "dev:generate": "run-p -l dev:generate:graphql",
    "dev:vite": "vite",
    "dev": "run-p -l dev:vite dev:generate"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@mdi/js": "^7.4.47",
    "@paralleldrive/cuid2": "^2.2.2",
    "@vue/apollo-composable": "4.0.0-beta.12",
    "@vueuse/core": "^10.7.1",
    "@vueuse/shared": "^10.7.1",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "pinia": "^2.1.7",
    "vue": "^3.4.3",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.0",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@graphql-codegen/typescript-vue-apollo": "^4.1.0",
    "@playwright/test": "^1.40.1",
    "@rushstack/eslint-patch": "^1.6.1",
    "@tsconfig/node20": "^20.1.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.6",
    "@vitejs/plugin-vue": "^5.0.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.5.1",
    "@zammad-coding-test/eslint-config-standards": "workspace:*",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^23.0.1",
    "less": "^4.2.0",
    "nodemon": "^3.0.2",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.1",
    "vue-tsc": "^1.8.27"
  }
}
